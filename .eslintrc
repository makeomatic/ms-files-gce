{
  "extends": "airbnb-base",
  "parser": "babel-eslint",
  "rules": {
    "no-param-reassign": [2, {"props": false}],
    "prefer-arrow-callback": 0,
    "max-len": [2, 180],
    "no-underscore-dangle": 0
  },
  "plugins": [
    "mocha"
  ],
  "ecmaFeatures": {
    "classes": true
  },
  "env": {
    "mocha": true
  }
}